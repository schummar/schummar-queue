{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES2015",
    "lib": ["ES2015"],
    "allowJs": true,
    "jsx": "react",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
